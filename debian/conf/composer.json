{
    "name": "deb/clientzone",
    "description": "ClientZone for FlexiBee",
    "version": "1.1",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "config": {
        "vendor-dir": "/var/lib/clientzone"
    },
    "require": {
        "deb/flexipeehp": "*",
        "deb/ease-framework": "*",
        "deb/ease-bricks": "*",
        "deb/flexipeehp-bricks": "*"
    },
    "autoload": {
        "psr-4": {
            "ClientZone\\": "ClientZone/",
            "ClientZone\\ui\\": "ClientZone/ui/",
            "ClientZone\\whplugins\\": "ClientZone/whplugins/",
            "ClientZone\\orderplugins\\": "ClientZone/orderplugins/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "/usr/share/php/Ease",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "/usr/share/php/EaseBricks",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "/usr/share/php/FlexiPeeHP",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "/usr/share/php/FlexiPeeHPBricks",
            "options": {
                "symlink": true
            }
        }
    ]
}
